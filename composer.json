{
    "name": "blackshadev/littledivelogserver",
    "type": "project",
    "description": "A backend to log your dives.",
    "keywords": [
        "divelog",
        "api"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "dms/phpunit-arraysubset-asserts": "^0.2.1",
        "fideloper/proxy": "^4.2",
        "fzaninotto/faker": "^1.5",
        "guzzlehttp/guzzle": "^7.01",
        "laravel/framework": "^9.0",
        "laravel/scout": "^9.0",
        "laravel/tinker": "^2.0",
        "lcobucci/jwt": "^4.1",
        "php-http/guzzle7-adapter": "^1.0",
        "spatie/once": "^3.0",
        "typesense/laravel-scout-typesense-driver": "^5.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-ignition": "^1.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.0",
        "symplify/easy-coding-standard": "^11.0.8"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "discard-changes": true,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Littledev\\Tauth\\": "packages/Littledev/Tauth/src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
